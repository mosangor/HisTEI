        | tei_respStmt)*)
      | ((tei_title | tei_model.ptrLike | tei_idno)+,
         (tei_textLang
          | tei_author
          | tei_editor
          | tei_meeting
          | tei_respStmt)*)
      | (tei_authority, tei_idno))?,
     tei_model.noteLike*,
     (tei_edition,
      (tei_idno
       | tei_model.ptrLike
       | tei_editor
       | tei_sponsor
       | tei_funder
