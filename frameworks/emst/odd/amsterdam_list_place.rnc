    (tei_state+
     | (tei_model.headLike*,
        tei_model.pLike+,
        (tei_model.noteLike | tei_model.biblLike)*)
     | (tei_model.labelLike
        | tei_model.noteLike
        | tei_model.biblLike)*),
    tei_att.global.attributes,
    tei_att.datable.attributes,
    tei_att.editLike.attributes,
    tei_att.typed.attributes,
    tei_att.naming.attributes,
    empty
  }
