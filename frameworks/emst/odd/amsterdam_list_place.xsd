      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="tei_att.dimensions.attribute.extent">
    <xs:attribute name="extent" type="ns1:tei_data.text">
      <xs:annotation>
